<?xml version='1.0' encoding='UTF-8' ?>

<!ELEMENT Gauge (Container? , Update? , Image? , Size? , Element* , Mouse?)>

<!ATTLIST Gauge  Name    CDATA  #REQUIRED
                 Version CDATA  #FIXED "1.0" >


<!ELEMENT Container EMPTY >


<!ELEMENT Update EMPTY >

<!ATTLIST Update  Frequency CDATA  #IMPLIED >


<!ELEMENT Image (Axis? , Nonlinearity?) >

<!ATTLIST Image  Name            CDATA  #REQUIRED
                 ImageSizes      CDATA  #IMPLIED
                 DegreesPointsTo CDATA  '0'
                 Bright          (Yes | No )  'No'
                 PointsTo        (East | South | West | North )  'East'
                 UseTransparency (Yes | No )  'Yes'
                 Luminous        (Yes | No )  'No'
                 Alpha           (Yes | No )  'No'
                 NoBilinear      (Yes | No )  'No' >


<!ELEMENT Axis EMPTY >

<!ATTLIST Axis  X CDATA  #REQUIRED
                Y CDATA  #REQUIRED >


<!ELEMENT Nonlinearity (Item, Item+) >


<!ELEMENT Item EMPTY >

<!ATTLIST Item  Value   CDATA  #REQUIRED
                Degrees CDATA  #IMPLIED
				Radians CDATA  #IMPLIED
                X       CDATA  #IMPLIED
                Y       CDATA  #IMPLIED >


<!ELEMENT Size EMPTY>

<!ATTLIST Size  X CDATA  #REQUIRED
                Y CDATA  #REQUIRED >


<!ELEMENT Element ((Element | Image | MaskImage | Text | Position | Size | Visible | Select | SelectRange | Shift | Rotate | Clip | Failures)*) >


<!ELEMENT MaskImage (Axis? , Nonlinearity?) >

<!ATTLIST MaskImage  Name            CDATA  #REQUIRED
                     ImageSizes      CDATA  #IMPLIED
                     DegreesPointsTo CDATA  '0'
                     Bright          (Yes | No )  'No'
                     PointsTo        (East | South | West | North )  'East'
                     UseTransparency (Yes | No )  'Yes'
                     Luminous        (Yes | No )  'No'
                     Alpha           (Yes | No )  'No'
                     NoBilinear      (Yes | No )  'No' >


<!ELEMENT Text (String , Hilight? , Failures? , Axis? , Nonlinearity?) >

<!ATTLIST Text  X               CDATA  #REQUIRED
                Y               CDATA  #REQUIRED
                Length          CDATA  #REQUIRED
                Fixed           (Yes | No )  'No'
                WidthFit        (Yes | No )  'No'
                Font            CDATA  #REQUIRED
                FontWeight      CDATA  '400'
                FontHeight      CDATA  '0'
                Charset         CDATA  #IMPLIED
                Attributes      CDATA  'Normal'
                Adjust          (Left | Center | Right )  'Left'
                VerticalAdjust  (Top | Center | Bottom )  'Top'
                Multiline       (Yes | No )  #IMPLIED
                Color           CDATA  #IMPLIED
                BackgroundColor CDATA  #IMPLIED
                HilightColor    CDATA  #IMPLIED
                Bright          (Yes | No )  'No'
                UseTransparency (Yes | No )  'Yes'
                Luminous        (Yes | No )  'No'
                Alpha           (Yes | No )  'No'
                NoBilinear      (Yes | No )  'No'
                DegreesPointsTo CDATA  '0'
                PointsTo        (East | South | West | North )  'East' >


